    "#contoh 2 penjumlahan sebuah barisan angka\n",
    "def jumlah(angka):\n",
    "    total = 0\n",
    "    for x in angka:\n",
    "        total += x\n",
    "    return total\n",
    "\n",
    "print(\"jumlah: \",jumlah((8,3,1,4,5)))"
